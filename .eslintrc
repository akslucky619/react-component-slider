{
  "extends": "airbnb",
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": "6",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": "true",
      "modules": "true"
    }
  },
  "globals": {
    "window": true
  },
  "rules": {
    "arrow-body-style": "off"
  }
}
